<?xml version="1.0"?>
<robot name="irp6p"
       xmlns:sensor="http://playerstage.sourceforge.net/gazebo/xmlschema/#sensor"
       xmlns:controller="http://playerstage.sourceforge.net/gazebo/xmlschema/#controller"
       xmlns:xacro="http://ros.org/wiki/fixme"
       xmlns:interface="http://playerstage.sourceforge.net/gazebo/xmlschema/#interface">

  <property name="M_PI" value="3.14159"/>

  <link name="world">
    <inertial>
      <mass value="0.05" />
      <origin xyz="-0.0 0.0 -10.0" />
      <inertia ixx="1.0" ixy="0.0" ixz="0.0"
               iyy="1.0" iyz="0.0"
               izz="1.0" />
    </inertial>

    <collision>
      <origin xyz="0 0 0.15" rpy="0 0 0" />
      <geometry>
        <box size="0.4 0.4 0.3" />
      </geometry>
    </collision>
  </link>

  <joint name="base_joint" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
  </joint>

  <link name="base_link">
    <inertial>
      <mass value="0" />
      <origin xyz="-0.0 0.0 0.0" />
      <inertia ixx="1.0" ixy="0.0" ixz="0.0"
               iyy="1.0" iyz="0.0"
               izz="1.0" />
    </inertial>

    <collision>
      <origin xyz="0 0 0.15" rpy="0 0 0" />
      <geometry>
        <box size="0.4 0.4 0.3" />
      </geometry>
    </collision>
  </link>
  
  <joint name="joint1" type="revolute">
    <origin rpy="0 0 0" xyz="0.0 0.0 0.3"/>
    <axis xyz="0 0 1"/>
    <limit lower="-2.96705973" upper="2.96705973" effort="10.0" velocity="10.0" />
    <parent link="base_link"/>
    <child link="link1"/>
  </joint>

  <link name="link1">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0.0 0.0 0.0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    
    <visual>
      <origin rpy="-1.57 0 0.0" xyz="0 0 0.4"/>
      <geometry>
        <mesh filename="package://irp6_defs/meshes/link1.3ds" />
      </geometry>
    </visual>
    
    <collision>
      <origin rpy="-1.57 0 0.0" xyz="0 0 0.4"/>
      <geometry>
        <mesh filename="package://irp6_defs/meshes/link1.stlb" />
      </geometry>
    </collision>
  </link>

  <joint name="joint2" type="revolute">
    <origin rpy="${-0.5 * M_PI} 0 0" xyz="0.0 0.0 0.4"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.8" upper="-0.872664626" effort="10.0" velocity="10.0" />
    <parent link="link1"/>
    <child link="link2"/>
  </joint>

  <link name="link2">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0.0 0.0 0.0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    
    <visual>
      <origin rpy="0 0 0.0" xyz="0.45 0.0 0.0"/>
      <geometry>
        <mesh filename="package://irp6_defs/meshes/link2.3ds" />
      </geometry>
    </visual>
    
    <collision>
      <origin rpy="0 0 0.0" xyz="0.2 0.0 0.0"/>
      <geometry>
        <box size="0.4 0.10 0.2"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint3" type="revolute">
    <origin rpy="0 0 0.0" xyz="0.45 0.0 0.0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.3" upper="0.5" effort="10.0" velocity="10.0" />
    <parent link="link2"/>
    <child link="link3"/>
  </joint>

  <link name="link3">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0.0 0.0 0.0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    
    <visual>
      <origin rpy="0 0 1.57" xyz="0 0.67 0.0"/>
      <geometry>
        <mesh filename="package://irp6_defs/meshes/link3.3ds" />
      </geometry>
    </visual>
    
    <collision>
      <origin rpy="0 0 1.57" xyz="0 0.67 0.0"/>
      <geometry>
        <mesh filename="package://irp6_defs/meshes/link3.stlb" scale="1.0 1.0 1.3" />
      </geometry>
    </collision>
  </link>

  <joint name="joint4" type="revolute">
    <origin rpy="0 0 0" xyz="0.0 0.67 0.0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.57" upper="1.57" effort="10.0" velocity="10.0" />
    <parent link="link3"/>
    <child link="link4"/>
  </joint>

  <link name="link4">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0.0 0.0 0.0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    
    <visual>
      <origin rpy="1.57 0 1.57" xyz="0.0 0.175 0.0"/>
      <geometry>
        <mesh filename="package://irp6_defs/meshes/link4.3ds" />
      </geometry>
    </visual>

    <collision>
      <origin rpy="1.57 0 1.57" xyz="0.0 0.175 0.0"/>
      <geometry>
        <mesh filename="package://irp6_defs/meshes/link4.stlb" />
      </geometry>
    </collision>
  </link>

  <joint name="joint5" type="revolute">
    <origin rpy="${-0.5 * M_PI} 0 0" xyz="0.0 0.0 0.0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.5" upper="5.14" effort="10.0" velocity="10.0" />
    <parent link="link4"/>
    <child link="link5"/>
  </joint>

  <link name="link5">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0.0 0.0 0.0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>

    <visual>
      <origin rpy="1.57 0 0.0" xyz="0.0 0.0 0.19"/>
      <geometry>
        <mesh filename="package://irp6_defs/meshes/link5.3ds" />
      </geometry>
    </visual>

    <collision>
      <origin rpy="1.57 0 0.0" xyz="0.0 0.0 0.19"/>
      <geometry>
        <mesh filename="package://irp6_defs/meshes/link5.stlb" />
      </geometry>
    </collision>
  </link>

  <joint name="joint6" type="revolute">
    <origin rpy="${0.5 * M_PI} 0 0" xyz="0.0 -0.15 0.19"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.0" upper="2.9" effort="10.0" velocity="10.0" />
    <parent link="link5"/>
    <child link="link6"/>
  </joint>
  
  <link name="link6">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0.0 0.0 0.0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>

    <visual>
      <origin rpy="0 0 0.0" xyz="0 0.0 0.0"/>
      <geometry>
         <mesh filename="package://irp6_defs/meshes/link6.3ds" />
      </geometry>
    </visual>

    <collision>
      <origin rpy="0.0 0 0.0" xyz="0 0.0 -0.05"/>
      <geometry>
        <cylinder radius="0.1" length="0.10" />
      </geometry>
    </collision>
  </link>

  <joint name="gripper_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0.0 0.0 0.06"/>
    <parent link="link6"/>
    <child link="griper_link"/>
  </joint>
  
  <link name="griper_link">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0.0 0.0 0.0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>

    <visual>
      <origin rpy="0 0 0.0" xyz="0 0.0 0.0"/>
      <geometry>
         <box size="0.05 0.1 0.12" />
      </geometry>
    </visual>

    <collision>
      <origin rpy="0.0 0 0.0" xyz="0 0.0 0.0"/>
      <geometry>
        <box size="0.05 0.2 0.12" />
      </geometry>
    </collision>
  </link>

